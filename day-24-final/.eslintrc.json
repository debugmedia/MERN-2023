{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes":"off",
        "linebreak-style":"off",
        "import/extensions":"off",
        "import/prefer-default-export":"off",
        "import/no-extraneous-dependencies":"off",
        "indent":["error", 4]
    }
}
